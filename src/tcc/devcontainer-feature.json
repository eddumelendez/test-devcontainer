{
    "name": "Testcontainers Cloud",
    "id": "tcc",
    "version": "0.0.2",
    "description": "Testcontainers Cloud feature",
    "options": {
        "version": {
            "type": "string",
            "default": "latest",
            "description": "Specific version of Testcontainers Cloud Client to use"
        }
    },
    "postStartCommand": "nohup /usr/local/share/tcc-agent > /usr/local/share/tcc-agent.log 2>&1 &",
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}